using System;

namespace While_loop_revision
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("**********While loop*************");

            int counter = 0;

            while (counter <10)
            {
                if(counter ==4)
                {
                    continue;
                }
                Console.WriteLine("Counter is" + counter);
                counter++;
            }
        }
    }
}
