using UnityEngine;

public class Distance : MonoBehaviour
{
    Vector3 v1, v2;
    // Start is called before the first frame update
    void Start()
    {
        v1 = new Vector3(2, 4, transform.position.z);
        v2 = new Vector3(4, 6, transform.position.z);
