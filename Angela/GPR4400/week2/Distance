        float b = 6 - 4;
        float c = Mathf.Sqrt(Mathf.Pow(a, 2) + Mathf.Pow(b, 2));
        Debug.Log("" + c);
    }

    // Update is called once per frame
    void Update()
    {
        
    }
}
