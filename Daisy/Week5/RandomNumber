using System;

namespace RandomNumber
{
    class Program
    {
        static void Main(string[] args)
        {
            Random rand = new Random();

            Console.WriteLine(rand.Next()); // random numver between -2billion and +2billion
            Console.WriteLine(rand.Next(5)); // random number up to 5
            Console.WriteLine(rand.Next(3, 1432)); // random number from 3 to 1431 (misses out last number)
            Console.WriteLine(rand.NextDouble());
            
        }
    }
}
